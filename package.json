{
  "name": "cyclos4-ui",
  "displayName": "Cyclos 4 UI",
  "description": "A modern and open web interface to Cyclos, which can be easily customized and self-hosted",
  "icon": "cyclos.png",
  "version": "1.1.0-dev",
  "license": "MIT",
  "author": {
    "name": "Cyclos development team",
    "email": "info@cyclos.org",
    "url": "https://www.cyclos.org"
  },
  "scripts": {
    "ng": "ng",
    "ng-openapi-gen": "ng-openapi-gen",
    "ng-translation-gen": "ng-translation-gen",
    "merge-translations": "npm run ng-translation-gen -- --merge",
    "ngx-extractor": "ngx-extractor",
    "svgo": "svgo",
    "generate-icons": "node tools/generate-icons.js",
    "generate": "npm run ng-openapi-gen && npm run ng-translation-gen && npm run generate-icons",
    "optimize-svgs": "npm run svgo -- src/icons/*.svg --config='{ \"plugins\": [{ \"removeViewBox\": false }] }'",
    "start": "npm run generate && npm run ng -- serve --proxy-config proxy.json",
    "build": "npm run generate && npm run optimize-svgs && npm run merge-translations && npm run ng -- build --prod --progress=false",
    "lint": "npm run ng lint"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.6",
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@types/autosize": "^3.0.6",
    "@types/chart.js": "^2.7.53",
    "@types/downloadjs": "^1.4.1",
    "@types/googlemaps": "^3.36.4",
    "@types/lodash": "^4.14.134",
    "@types/lscache": "^1.0.29",
    "autosize": "^4.0.2",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "core-js": "^2.6.9",
    "downloadjs": "^1.4.7",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "lscache": "^1.3.0",
    "moment-mini-ts": "^2.20.1",
    "ng-event-source": "^1.0.14",
    "ngx-bootstrap": "^4.3.0",
    "ngx-lightbox": "^1.2.0",
    "popper.js": "^1.15.0",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@types/node": "^10.14.9",
    "codelyzer": "~4.5.0",
    "ng-openapi-gen": "^0.4.0",
    "ng-translation-gen": "^0.5.0",
    "svgo": "^1.2.2",
    "tar": "^4.4.10",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "xmldom": "^0.1.27"
  }
}
