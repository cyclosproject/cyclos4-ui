{
  "name": "cyclos4-ui",
  "displayName": "Cyclos 4 UI",
  "description": "A modern and open web interface to Cyclos, which can be easily customized and self-hosted",
  "icon": "cyclos.png",
  "version": "2.0.0-dev",
  "license": "MIT",
  "author": {
    "name": "Cyclos development team",
    "email": "info@cyclos.org",
    "url": "https://www.cyclos.org"
  },
  "scripts": {
    "ng": "ng",
    "ng-openapi-gen": "ng-openapi-gen",
    "ng-translation-gen": "ng-translation-gen",
    "merge-translations": "npm run ng-translation-gen -- --merge",
    "svgo": "svgo",
    "generate-icons": "node tools/generate-icons.js",
    "generate": "npm run ng-openapi-gen && npm run ng-translation-gen && npm run generate-icons",
    "optimize-svgs": "npm run svgo -- src/icons/*.svg --config='{ \"plugins\": [{ \"removeViewBox\": false }] }'",
    "start": "npm run generate && npm run ng -- serve --host 0.0.0.0 --proxy-config proxy.json",
    "build": "npm run generate && npm run optimize-svgs && npm run merge-translations && npm run ng -- build --prod --progress=false --named-chunks",
    "lint": "npm run ng -- lint",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.3",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "^9.1.3",
    "@angular/compiler": "^9.1.3",
    "@angular/core": "^9.1.3",
    "@angular/forms": "^9.1.3",
    "@angular/platform-browser": "^9.1.3",
    "@angular/platform-browser-dynamic": "^9.1.3",
    "@angular/router": "^9.1.3",
    "@googlemaps/loader": "0.0.7",
    "@types/autosize": "^3.0.7",
    "@types/chart.js": "^2.9.19",
    "@types/downloadjs": "^1.4.1",
    "@types/googlemaps": "^3.39.3",
    "@types/lodash": "^4.14.150",
    "@types/lscache": "^1.3.0",
    "@types/markerclustererplus": "^2.1.33",
    "@types/rangy": "0.0.33",
    "@zxing/ngx-scanner": "^3.0.0",
    "autosize": "^4.0.2",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "core-js": "^2.6.11",
    "downloadjs": "^1.4.7",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.0",
    "lodash": "^4.17.15",
    "lscache": "^1.3.0",
    "moment-mini-ts": "^2.20.1",
    "ng-event-source": "^1.0.14",
    "ngx-bootstrap": "^5.6.1",
    "ngx-gallery-9": "^1.0.6",
    "popper.js": "^1.16.1",
    "rangy": "^1.3.0",
    "rxjs": "^6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.3",
    "@angular/cli": "^9.1.3",
    "@angular/compiler-cli": "^9.1.3",
    "@angular/language-service": "^9.1.3",
    "@types/node": "^12.12.37",
    "codelyzer": "^5.1.2",
    "ng-openapi-gen": "^0.10.0",
    "ng-translation-gen": "^0.5.0",
    "svgo": "^1.3.2",
    "tar": "^4.4.13",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "3.8.3",
    "xmldom": "^0.1.31"
  }
}
